            id="org.netxms.ui.eclipse.serverconfig.views.PersistentStorageView"
            name="Persistent Storage">
      </view>
      <view
            allowMultiple="false"
            category="org.netxms.ui.eclipse.viewcategory.config"
            class="org.netxms.ui.eclipse.serverconfig.views.NotificationChannelView"
            id="org.netxms.ui.eclipse.serverconfig.views.NotificationChannelView"
            name="Notification Channel"
            restorable="true">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            name="%command.name.ExportConfig"
