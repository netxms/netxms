   <extension
         point="org.eclipse.ui.activities">
      <activity
            id="org.netxms.ui.eclipse.console.activity.SendNotification"
            name="Send notification">
         <enabledWhen>
            <with
                  variable="org.netxms.access.SendNotification">
               <equals
                     value="true">
               </equals>
            </with></enabledWhen>
      </activity>
      <activityPatternBinding
            activityId="org.netxms.ui.eclipse.console.activity.SendNotification"
            isEqualityPattern="true"
            pattern="org.netxms.webui.core/org.netxms.ui.eclipse.console.actions.send_notification">
      </activityPatternBinding>
   </extension>
   <extension
         point="org.eclipse.core.expressions.propertyTesters">
      <propertyTester
            class="org.netxms.ui.eclipse.console.ServerComponentPropertyTester"
            id="org.netxms.ui.eclipse.console.ServerComponentPropertyTester"
            namespace="org.netxms.ui.eclipse"
            properties="isComponentRegistered"
            type="java.lang.Object">
      </propertyTester>
   </extension>
   <extension
         point="org.eclipse.rap.ui.themes">
      <theme
            file="theme/classic.css"
            id="org.netxms.webui.design.theme.classic"
            name="Classic Theme">
      </theme>
      <theme
