<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension-point id="org.netxms.ui.eclipse.loginlisteners" name="NXMC Login Listeners" schema="schema/org.netxms.ui.eclipse.loginlisteners.exsd"/>
   <extension-point id="org.netxms.ui.eclipse.branding" name="NetXMS Console Branding" schema="schema/org.netxms.ui.eclipse.branding.exsd"/>
   <extension-point id="org.netxms.ui.eclipse.sessionproviders" name="Session Providers" schema="schema/org.netxms.ui.eclipse.sessionproviders.exsd"/>
   <extension-point id="org.netxms.ui.eclipse.colorproviders" name="Color Providers" schema="schema/org.netxms.ui.eclipse.colorproviders.exsd"/>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            name="%perspective.default"
            class="org.netxms.ui.eclipse.console.perspectives.ManagementPerspective"
            id="org.netxms.ui.eclipse.console.ManagementPerspective">
      </perspective>
   </extension>
   
   <extension
         point="org.eclipse.ui.views">
      <view
            allowMultiple="false"
            category="org.eclipse.ui"
            class="org.eclipse.ui.ExtensionFactory:progressView"
            icon="icons/pview.gif"
            id="org.eclipse.ui.views.ProgressView"
            name="%view.progress"
            restorable="true">
      </view>
      <category
            id="org.netxms.ui.eclipse.viewcategory.config"
            name="%category.config">
      </category>
      <category
            id="org.netxms.ui.eclipse.viewcategory.tools"
            name="%category.tools">
      </category>
   </extension>
   <extension point="org.eclipse.ui.preferencePages">
     <page name="%page.workbench"
           class="org.netxms.ui.eclipse.console.preferencepages.WorkbenchGeneralPrefs"
           id="org.eclipse.ui.preferencePages.Workbench">
     </page>
      <page category="org.eclipse.ui.preferencePages.Workbench"
            class="org.eclipse.ui.ExtensionFactory:perspectivesPreferencePage"
            id="org.eclipse.ui.preferencePages.Perspectives"
            name="%page.perspectives">
     </page>
     <page
            name="%page.decorators"
            category="org.eclipse.ui.preferencePages.Workbench"
            class="org.eclipse.ui.ExtensionFactory:decoratorsPreferencePage"
            id="org.eclipse.ui.preferencePages.Decorators">
     </page>
     <page
           class="org.netxms.ui.eclipse.console.preferencepages.RegionalSettingsPrefPage"
           id="org.netxms.ui.eclipse.preferencePages.RegionalSettings"
           name="%page.regionalSettings">
     </page>
     <page
           class="org.netxms.ui.eclipse.console.preferencepages.StatusColors"
           id="org.netxms.ui.eclipse.console.preferencepages.StatusColors"
           name="Status Colors">
     </page>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            description="Send notification"
            id="org.netxms.ui.eclipse.console.commands.send_notification"
            name="Send notification">
      </command>
      <command
            description="%command.PauseTrace"
            id="org.netxms.ui.eclipse.library.commands.pause_trace"
            name="%command.PauseTrace">
      </command>
      <command
            description="%command.description.Copy"
            id="org.netxms.ui.eclipse.library.commands.copy"
            name="%command.name.Copy">
      </command>
      <command
            description="%command.description.Refresh"
            id="org.netxms.ui.eclipse.library.commands.refresh"
            name="%command.name.Refresh">
      </command>
      <command
            description="%command.description.ExportCSV"
            id="org.netxms.ui.eclipse.library.commands.export_to_csv_selection"
            name="%command.name.ExportCSV">
      </command>
      <command
            description="%command.description.ExportAllCSV"
            id="org.netxms.ui.eclipse.library.commands.export_to_csv_all"
            name="%command.name.ExportAllCSV">
      </command>
      <category
            id="org.netxms.ui.eclipse.console.category.views"
            name="%category.name.Views">
      </category>
      <category
            id="org.netxms.ui.eclipse.console.category.tools"
            name="%category.name.Tools">
      </category>
      <command
            description="Toggle Filter"
            id="org.netxms.ui.eclipse.library.commands.show_filter"
            name="Toggle Filter">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.actionSets">
      <actionSet
            id="org.netxms.ui.eclipse.console.action_set"
            label="%actionSet.core"
            visible="true">
         <action
               class="org.netxms.ui.eclipse.console.actions.SendNotification"
               definitionId="org.netxms.ui.eclipse.console.commands.send_notification"
               id="org.netxms.ui.eclipse.console.actions.send_notification"
               label="Send notification"
               menubarPath="tools/additions">
         </action>
      </actionSet>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
      <key
            commandId="org.netxms.ui.eclipse.console.commands.send_notification"
            schemeId="org.netxms.ui.eclipse.defaultKeyBinding"
            sequence="M1+M3+S">
      </key>
      <key
            commandId="org.netxms.ui.eclipse.library.commands.show_filter"
            contextId="org.netxms.ui.eclipse.library.context.AbstractTraceView"
            schemeId="org.netxms.ui.eclipse.defaultKeyBinding"
            sequence="M1+F2">
      </key>
      <key
            commandId="org.netxms.ui.eclipse.library.commands.pause_trace"
            contextId="org.netxms.ui.eclipse.library.context.AbstractTraceView"
            schemeId="org.netxms.ui.eclipse.defaultKeyBinding"
            sequence="PAUSE">
      </key>
      <key
            commandId="org.netxms.ui.eclipse.library.commands.copy"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.netxms.ui.eclipse.defaultKeyBinding"
            sequence="M1+C">
      </key>
      <key
            commandId="org.netxms.ui.eclipse.library.commands.refresh"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.netxms.ui.eclipse.defaultKeyBinding"
            sequence="F5">
      </key>
      <key
            commandId="org.netxms.ui.eclipse.library.commands.export_to_csv_selection"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.netxms.ui.eclipse.defaultKeyBinding"
            sequence="M1+M3+X">
      </key>
      <key
            commandId="org.netxms.ui.eclipse.library.commands.export_to_csv_all"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.netxms.ui.eclipse.defaultKeyBinding"
            sequence="M1+M3+A">
      </key>
      <key
            commandId="org.eclipse.ui.file.closePart"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.netxms.ui.eclipse.defaultKeyBinding"
            sequence="M1+W">
      </key>
      <scheme
            description="%scheme.description"
            id="org.netxms.ui.eclipse.defaultKeyBinding"
            name="%scheme.name">
      </scheme>
   </extension>
   <extension
         point="org.eclipse.ui.contexts">
      <context
            id="org.netxms.ui.eclipse.library.context.AbstractTraceView"
            name="Abstract Trace View"
            parentId="org.eclipse.ui.contexts.window">
      </context>
   </extension>
   <extension
         point="org.eclipse.ui.presentationFactories">
      <factory
            class="org.netxms.ui.eclipse.console.themes.classic.ClassicPresentationFactory"
            id="org.netxms.ui.eclipse.console.factory.classic"
            name="Classic Presentation Factory">
