    DEFVAL { 5 }
    ::= { h3cSyslogSnmp 1 }

h3cSyslogLogbuffer OBJECT IDENTIFIER ::= { h3cSyslogObjects 5 }

h3cSyslogLogbufferChannel OBJECT-TYPE
