h3cSyslogLoghost OBJECT IDENTIFIER ::= { h3cSyslogObjects 7 }

h3cSyslogLoghostSourceInterface OBJECT-TYPE
    SYNTAX INTEGER
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
        "The source interface which sends message to loghost.
