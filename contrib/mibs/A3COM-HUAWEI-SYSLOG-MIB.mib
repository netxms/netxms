        h3cLogbufferIndex                    Integer32,
        h3cLogbufferCurrentMessages          Unsigned32,
        h3cLogbufferOverwrittenMessages      Counter32,
        h3cLogbufferDroppedMessages          Counter32
        }

h3cLogbufferIndex OBJECT-TYPE
    SYNTAX Integer32
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
