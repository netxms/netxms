            id="org.netxms.ui.eclipse.library.commands.show_filter"
            name="Toggle Filter">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.actionSets">
      <actionSet
            id="org.netxms.ui.eclipse.console.action_set"
            label="%actionSet.core"
            visible="true">
